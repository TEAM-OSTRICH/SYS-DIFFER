{
  "name": "Chrisdiffer",
  "productName": "Chrisdiffer",
  "version": "1.0.0",
  "description": "Schema differ",
  "main": "./public/electron.js",
  "scripts": {
    "electron": "electron .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "webpack --config webpack.config.js",
    "prestart": "npm run build",
    "start": "electron public/electron.js",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --port 3000 --config=./webpack.config.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/database.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Chrisdiffer --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/win/database.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Chrisdiffer\"",
    "package-linux": "electron-packager . Chrisdiffer --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/png/database.png --prune=true --out=release-builds"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Ge Sun, Lee Rogers, Kevin Mui",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "body-parser": "^1.18.3",
    "d3": "^5.7.0",
    "deep-diff": "^1.0.2",
    "electron-storage": "^1.0.7",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "pg": "^7.6.0",
    "pg-promise": "^8.5.1",
    "postgre-sql": "^1.18.0",
    "promise-pg": "^0.1.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.79.0",
    "typeface-roboto": "0.0.54",
    "webpack-dashboard": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "electron": "^3.0.9",
    "electron-installer-dmg": "^2.0.0",
    "electron-is-dev": "^1.0.1",
    "electron-packager": "^12.2.0",
    "electron-settings": "^2.2.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "file-loader": "^2.0.0",
    "grunt": "^1.0.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "json-loader": "^0.5.7",
    "react-test-renderer": "^16.6.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "yarn": "^1.12.1"
  },
  "homepage": "./"
}
